# Track ffmpeg binaries with Git LFS
video_vault/externals/ffmpegs/windows/ffmpeg.exe filter=lfs diff=lfs merge=lfs -text
video_vault/externals/ffmpegs/darwin/ffmpeg filter=lfs diff=lfs merge=lfs -text
video_vault/externals/ffmpegs/linux/ffmpeg filter=lfs diff=lfs merge=lfs -text
# Track other large binary files that might be added in the future
*.exe filter=lfs diff=lfs merge=lfs -text
VideoVault filter=lfs diff=lfs merge=lfs -text
